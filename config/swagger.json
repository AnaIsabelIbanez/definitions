{
    "swagger": "2.0",
    "info": {
        "version": "1.0.0",
        "title": "Swagger Productstore",
        "license": {
            "name": "MIT"
        }
    },
    "host": "petstore.swagger.io",
    "basePath": "/api",
    "schemes": [
        "http"
    ],
    "consumes": [
        "application/json"
    ],
    "produces": [
        "application/json"
    ],
    "paths": {
        "/login": {
            "post": {
                "$ref": "./paths/login.json"
            }
        },
        "/product": {
            "get": {
                "$ref": "./paths/product.json"
            }
        },
        "/product/{id}": {
            "get": {
                "$ref": "./paths/productId.json"
            }
        },
        "/return": {
            "get": {
                "$ref": "./paths/return.json"
            }
        },
        "/return/{id}": {
            "get": {
                "$ref": "./paths/returnId.json"
            }
        },
        "/return/{id}/line": {
            "get": {
                "$ref": "./paths/line.json"
            }
        },
        "/return/line/{id}": {
            "patch": {
                "$ref": "./paths/lineId.json"
            }
        },
        "/brand": {
            "get": {
                "$ref": "./paths/keyValueList.json"
            }
        },
        "/category": {
            "get": {
                "$ref": "./paths/keyValueList.json"
            }
        },
        "/size": {
            "get": {
                "$ref": "./paths/keyValueList.json"
            }
        },
        "/color": {
            "get": {
                "$ref": "./paths/keyValueList.json"
            }
        },
        "/return_type": {
            "get": {
                "$ref": "./paths/keyValueList.json"
            }
        },
        "/return_reason": {
            "get": {
                "$ref": "./paths/keyValueList.json"
            }
        },
        "/return_subreason": {
            "get": {
                "$ref": "./paths/keyValueList.json"
            }
        },
        "/warehouse_name": {
            "get": {
                "$ref": "./paths/keyValueList.json"
            }
        }
    }
}